                <div class="row g-5 justify-content-center">
                    <h1 class="l-space ">SignUp Modification</h1>
                <div class="col-7">
                              <form class="row p-3 bg-primary-subtle" method="post"  >
                                  <div class="col">First name
                                      <input type="text" class="form-control" placeholder="Enter first name" name="first_name" value="<%=movies1.first_name %>">
                                    </div>
                                    <div class="col">Last name
                                      <input type="text" class="form-control" placeholder="Enter last name" name="last_name" value="<%=movies1.last_name %>">
                                    </div>
                                  <div class="col-md-12">
                                    <label for="inputEmail4" class="form-label">Email</label>
                                    <input type="email" class="form-control" id="inputEmail4" name="email_id" value="<%=movies1.email_id %>">
                                  </div>
                                  <div class="col-md-6">
                                    <label for="inputPassword4" class="form-label">Password</label>
                                    <input type="password" class="form-control" id="inputPassword4" name="password" value="<%=movies1.password %>">
                                  </div>
                                  <div class="col-md-6">
                                    <label for="inputPassword5" class="form-label">Re Enter Password</label>
                                    <input type="password" class="form-control" id="inputPassword5" value="<%=movies1.repeat_password %>" >
                                  </div>
                                  <div class="col-12">
                                    <label for="inputAddress" class="form-label">Address</label>
                                    <input type="text" class="form-control" id="inputAddress" placeholder="1234 Main St" name="address" value="<%=movies1.address %>">
                                  </div>
                                  <div class="col-12">
                                    <label for="tel" class="form-label">Tel</label>
                                    <input type="text" class="form-control" id="tel" placeholder="+91******" name="tel" value="<%=movies1.tel %>">
                                  </div>
                                  <!-- <div class="col-md-6">
                                    <label for="inputCity" class="form-label">City</label>
                                    <input type="text" class="form-control" id="inputCity" name="city" value="<%=movies1.city %>">
                                  </div> -->
